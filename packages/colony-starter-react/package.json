{
  "name": "@colony/colony-starter-react",
  "version": "1.0.0-beta.11",
  "description": "A simple starter project for building with Colony",
  "keywords": [
    "colony",
    "boilerplate",
    "react"
  ],
  "license": "MIT",
  "files": [
    "/",
    ".gitignore"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start-ganache": "colony service start-ganache",
    "deploy-contracts": "colony service deploy-contracts",
    "start-trufflepig": "colony service start-trufflepig",
    "seed-network": "colony service seed-network",
    "colony-setup": "colony service colony-setup",
    "build": "react-scripts build",
    "start": "react-scripts start",
    "test": "react-scripts test --coverage --updateSnapshot",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@colony/colony-cli": "^1.0.0-beta.6",
    "@colony/colony-js-client": "^1.12.0",
    "@colony/colony-js-contract-loader-http": "^1.12.0",
    "@colony/purser-metamask": "^2.2.0",
    "@colony/purser-software": "^1.2.4",
    "buffer": "^5.2.1",
    "ipfs": "^0.35.0",
    "ipfs-repo": "^0.26.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "web3": "^1.0.0-beta.55",
    "web3-utils": "^1.0.0-beta.55"
  },
  "devDependencies": {
    "node-sass": "^4.9.0",
    "react-scripts": "^2.1.8",
    "react-test-renderer": "^16.8.6"
  },
  "engines": {
    "node": ">=10.12.0 <12.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
